<%= simple_form_for employee, url: admins_employees_path do |f| %>
  <div class="form-inputs">
    <%= f.input :name, autofocus: true %>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :password_confirmation %>
  </div>
  <div class="form-actions d-grid">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>
